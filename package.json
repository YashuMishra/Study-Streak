{
  "name": "studystreak",
  "version": "1.0.0",
  "description": "A comprehensive study streak management app for competitive exam aspirants with target tracking, Pomodoro timer, note-making, analytics, and community features focused on building consistent study habits",
  "private": false,
  "type": "module",
  "author": {
    "name": "Yash Raj",
    "email": "yash.raj@example.com",
    "url": "https://github.com/YashRaj"
  },
  "license": "MIT",
  "homepage": "https://yashumishra.github.io/Study-Streak/",
  "repository": {
    "type": "git",
    "url": "https://github.com/YashRaj/studystreak.git"
  },
  "bugs": {
    "url": "https://github.com/YashRaj/studystreak/issues"
  },
  "keywords": [
    "study-streak",
    "study-tracker",
    "pomodoro-timer",
    "note-taking",
    "student-productivity",
    "exam-preparation",
    "streak-tracking",
    "habit-building",
    "upsc",
    "jee",
    "neet",
    "competitive-exams",
    "react",
    "typescript",
    "tailwindcss",
    "study-management",
    "education",
    "learning"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "test:watch": "vitest",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "npm run typecheck && npm run test",
    "deploy": "gh-pages -d dist"
  },
